[user]
	email = netletic@gmail.com
	name = netletic
[init]
	defaultBranch = main
[pull]
	rebase = false
[color]
	ui = true
[merge]
	conflictstyle = diff3
[core]
	editor = nvim
	pager = diff-so-fancy | less --tabs=4 -RFX
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
